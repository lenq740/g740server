<form name="%form%" caption="Журнал выполнения плановых работ">
	<rowsets>
		<rowset name="sysextlog" paginator.count="300">
		</rowset>
	</rowsets>
	<panels>
		<panel rowset="sysextlog">
			<panel type="grid">
				<toolbar>
					<request name="refresh"/>
				</toolbar>
				<field name="icon"/>
				<field name="d"/>
				<field name="tstart" caption="с" len="5"/>
				<field name="tend" caption="по" len="5"/>
				<field name="name" caption="Процесс"/>
				<field name="message"/>
			</panel>
		</panel>
	</panels>
</form>
